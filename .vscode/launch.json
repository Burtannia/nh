    "version": "0.2.0",
    "configurations": [
        {
            "name": "nh update",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "nh",
            "args": [
                "update",
                "-R",
                "--dry-run",
                "/home/ayats/Documents/dotfiles"
            ],
        },
        {
            "name": "nh switch",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "nh",
            "args": [
                "switch",
                "/home/ayats/Documents/dotfiles",
                "--dry-run",
                // "-L"
            ],
        },
        {
            "name": "nh repl",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "nh",
            "args": [
                "repl",
                "/home/ayats/Documents/dotfiles"
            ],
        },
        {
            "name": "nh search",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "nh",
            "args": [
                "search",
                "zfs"
            ],
        },
        {
            "name": "nh gcr-clean",
            "type": "python",
            "request": "launch",
            "console": "internalConsole",
            "module": "nh",
            "args": [
                "gcr-clean",
                "--age",
                "7d",
                "--dry-run"
            ],
        },
        {
            "name": "Debug utils",
            "type": "python",
            "request": "launch",
            "module": "nh.utils",
            "console": "internalConsole"
        }
    ]
}
